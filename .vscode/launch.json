{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "chrome",
      "request": "attach",
      "name": "Attach to Chrome",
      "port": 9222,
      "url": "http://localhost:4200/*",
      "webRoot": "${workspaceFolder}",
      "sourceMapPathOverrides": {
        //"myappname" is from the environment.js "modulePrefix" property.
        "rarwe/*": "${workspaceFolder}/app/*"
      }
    }
  ]
}
